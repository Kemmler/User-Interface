operation setbuttons
params
numeric pPagecount: IN
numeric pLastPage: IN
numeric pPageno: IN
endparams
	fieldsyntax UPREV.UBUTTONDEFFRM, "HID"
	fieldsyntax UNEXT.UBUTTONDEFFRM, "HID"
	fieldsyntax ULAST.UBUTTONDEFFRM, "HID"
	fieldsyntax UPAGENO.UBUTTONDEFFRM, "HID"
	fieldsyntax UFIRST.UBUTTONDEFFRM, "HID"
	fieldsyntax UGOPAGE.UBUTTONDEFFRM, "HID"
	fieldsyntax UJUMPPAGE.UBUTTONDEFFRM, "HID"

	UPAGENO.UBUTTONDEFFRM = ""
	
	if(pPagecount &gt; 0)
	fieldsyntax UPREV.UBUTTONDEFFRM, "DIM"
	fieldsyntax UNEXT.UBUTTONDEFFRM, "DIM"
	fieldsyntax ULAST.UBUTTONDEFFRM, "DIM"
	fieldsyntax UFIRST.UBUTTONDEFFRM, "DIM"
	fieldsyntax UCLEARDEF.UBUTTONDEFFRM, "DIM"
	fieldsyntax ULOADDEF.UBUTTONDEFFRM, "DIM"

		if(plastpage &gt; 1)
			fieldsyntax UGOPAGE.UBUTTONDEFFRM, "YDI"
			fieldsyntax UJUMPPAGE.UBUTTONDEFFRM, "YDI"
			if(ppageno&gt;= plastpage)
				fieldsyntax UPREV.UBUTTONDEFFRM, "YDI"
				fieldsyntax UFIRST.UBUTTONDEFFRM, "YDI"
			elseif(ppageno = 1)
				fieldsyntax UNEXT.UBUTTONDEFFRM, "YDI"
				fieldsyntax ULAST.UBUTTONDEFFRM, "YDI"
			else
				fieldsyntax UPREV.UBUTTONDEFFRM, "YDI"
				fieldsyntax UNEXT.UBUTTONDEFFRM, "YDI"
				fieldsyntax ULAST.UBUTTONDEFFRM, "YDI"
				fieldsyntax UFIRST.UBUTTONDEFFRM, "YDI"
			endif
		endif
		fieldsyntax UPAGENO.UBUTTONDEFFRM, "YDI"
		fieldsyntax UCLEARDEF.UBUTTONDEFFRM, "YDI"
		UPAGENO.UBUTTONDEFFRM = "Page %%$pageno$%%% of %%$lastpage$%%%"
	else
		fieldsyntax ULOADDEF.UBUTTONDEFFRM, "YDI"
	endif
end




