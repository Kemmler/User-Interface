<DAT name="UECOPERS" xml:space='preserve'>; The constants mySessionService, myDtd and myDtdMapping
; are assumed to be defined in the Component Defines Trigger.
partner operation loadpage 
variables
  xmlstream x
  numeric vError
  string vErrorContext
endvariables

  x= $profile$

  if ($status &gt;= 0)
    activate/stateless "&lt;mySessionService&gt;".loadpage(x, vError, vErrorContext, $vPageLimit$, &lt;PageSize&gt;, $page$, $lastpage$)
    if ($status = 0)                                     ; activate of SSV successful...

      if (vError = 0)                                    ; ...and the SSV processed sucessfully
        clear/e "&lt;$entName&gt;"
        xmlload x, "dtd:&lt;myDtd&gt;", "&lt;myDtdMapping&gt;"       ; load result set

        if ($status &lt; 0)
          $instancehandle-&gt;zProcError($procerrorcontext) ; report xmlLoad error
        endif

      else
        $instancehandle-&gt;zProcError(vErrorContext)       ; report errors in SSV processing

      endif

    else
      $instancehandle-&gt;zProcError($procerrorcontext)     ; report activate error
    endif

  else
     $instancehandle-&gt;zProcError($procerrorcontext)      ; report xmlSave error
  endif
call setbuttons
End ; operation loadPage

partner operation load 
variables
  xmlstream x
  numeric vError
  string vErrorContext
endvariables

  xmlsave x, "dtd:&lt;myDtd&gt;", "&lt;myDtdMapping&gt;"             ; capture retrieve-profile

  if ($status &gt;= 0)
    activate/stateless "&lt;mySessionService&gt;".load(x, vError, vErrorContext)

    if ($status = 0)                                     ; activate of SSV successful...

      if (vError = 0)                                    ; ...and the SSV processed sucessfully
        clear/e "&lt;$entName&gt;"
        xmlload x, "dtd:&lt;myDtd&gt;", "&lt;myDtdMapping&gt;"       ; load result set

        if ($status &lt; 0)
          $instancehandle-&gt;zProcError($procerrorcontext) ; report xmlLoad error
        endif

      else
        $instancehandle-&gt;zProcError(vErrorContext)       ; report errors in SSV processing

      endif

    else
      $instancehandle-&gt;zProcError($procerrorcontext)     ; report activate error
    endif

  else
     $instancehandle-&gt;zProcError($procerrorcontext)      ; report xmlSave error
  endif

End ; operation load

partner operation save 
variables
  xmlstream x
  numeric vError
  string vErrorContext
  numeric vOccNo
  numeric vStatus
endvariables

  vOccNo = $curocc(&lt;$entName&gt;)                           ; save current occurrence

  xmlsave x, "dtd:&lt;myDtd&gt;", "&lt;myDtdMapping&gt;"

  if ($status &gt;= 0)
    activate/stateless "&lt;mySessionService&gt;".save(x, vError, vErrorContext)

    if ($status = 0)                                     ; activate of SSV successful...

      if (vError != 0)                                   ; ...but the SSV processed unsuccessfully
        $instancehandle-&gt;zProcError(vErrorContext)       ; report errors in SSV processing
      endif

      clear/e "&lt;$entName&gt;"

      ; the xmlStream as returned by the SSV may contain flagged elements that did
      ; not pass validation in the SSV. These elements are flagged using the
      ; 'valerr' meta-attributes.
      ; On Error Triggers are fired upon loading the result set by xmlLoad:
      xmlload x, "dtd:&lt;myDtd&gt;", "&lt;myDtdMapping&gt;"

      if ($status &lt; 0)
        $instancehandle-&gt;zProcError($procerrorcontext)   ; report xmlload error
      endif

    else
      $instancehandle-&gt;zProcError($procerrorcontext)     ; report activate error
    endif

  else
    $instancehandle-&gt;zProcError($procerrorcontext)       ; report xmlsave error
  endif

  vStatus = $status
  setocc "&lt;$entName&gt;", vOccNo                            ; reset occurrence
  $status = vStatus                                      ; ignore setocc status

  if (vError != 0) $status = -1                          ; report error from SSV to caller

  return ($status)                                       ; report $status to caller

End ; operation save

