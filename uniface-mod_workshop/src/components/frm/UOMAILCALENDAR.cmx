;============================================================================================
entry lpGetMonthNumber

   Params
      String  pvMonth   : IN
      Numeric pvMonthNo : OUT
   EndParams

   Variables
      Numeric lvMonth
   EndVariables

   SelectCase pvMonth
      Case "J A N U A R Y"
         lvMonth = 1
      Case "F E B R U A R Y"
         lvMonth = 2
      Case "M A R C H"
         lvMonth = 3
      Case "A P R I L"
         lvMonth = 4
      Case "M A Y"
         lvMonth = 5
      Case "J U N E"
         lvMonth = 6
      Case "J U L Y"
         lvMonth = 7
      Case "A U G U S T"
         lvMonth = 8
      Case "S E P T E M B E R"
         lvMonth = 9
      Case "O C T O B E R"
         lvMonth = 10
      Case "N O V E M B E R"
         lvMonth = 11
      Case "D E C E M B E R"
         lvMonth = 12
   EndSelectCase

   pvMonthNo = lvMonth

   return(0)

end ; entry lpGetMonthNumber

;============================================================================
;============================================================================
entry lpMarkField

   Params
      String  pvNewSelDay   : IN
      Numeric pvNewSelWeek  : IN
      Numeric pvNewSelMonth : IN
   EndParams

   Variables
      Numeric lvDay
      Numeric lvMonth
      Date    lvNewDate
   EndVariables

   if ($CV_NEWSELDAY$ != "" &amp; $CV_NEWSELWEEK$ &gt; 0 &amp; $CV_NEWSELMONTH$ &gt; 0)
      setocc "MONTH.DUMMY", $CV_NEWSELMONTH$
      setocc "WEEK.DUMMY", $CV_NEWSELWEEK$
      fieldvideo $CV_NEWSELDAY$, "COL=DEF"
      $CV_NEWSELDAY$   = pvNewSelDay
      $CV_NEWSELWEEK$  = pvNewSelWeek
      $CV_NEWSELMONTH$ = pvNewSelMonth
      setocc "MONTH.DUMMY", $CV_NEWSELMONTH$
      setocc "WEEK.DUMMY", $CV_NEWSELWEEK$
      fieldvideo $CV_NEWSELDAY$, "COL=32"
   else
      $CV_NEWSELDAY$   = pvNewSelDay
      $CV_NEWSELWEEK$  = pvNewSelWeek
      $CV_NEWSELMONTH$ = pvNewSelMonth
      setocc "MONTH.DUMMY", $CV_NEWSELMONTH$
      setocc "WEEK.DUMMY", $CV_NEWSELWEEK$
      fieldvideo $CV_NEWSELDAY$, "COL=32"
   endif

   lvDay     = @$CV_NEWSELDAY$
   call lpGetMonthNumber(NAME.MONTH, lvMonth)
   lvNewDate = $date("%%lvDay%%%-%%lvMonth%%%-%%YEAR.DUMMY%%%")
   postmessage "UOCALDAYS", "NEWDATE", "%%lvNewDate%%%"

   return(0)

end ; entry lpMarkField</DAT>
