<DAT name="INIT" xml:space='preserve'>;+++ Initialise some stuff ...
$CV_CURSELDAY$   = ""
$CV_CURSELWEEK$  = ""
$CV_CURSELMONTH$ = ""
YEAR.DUMMY       = $date[Y]
call lpBuildCalendar(YEAR.DUMMY)

;+++ Set Window Position ...
$windowproperties($instancename, "STATE") = "STATE=NORMAL"
$windowproperties($instancename, "XPOSPERC&uSEP;YPOSPERC&uSEP;XSIZEPERC&uSEP;YSIZEPERC") = "XPOSPERC=0&uSEP;YPOSPERC=5&uSEP;XSIZEPERC=20&uSEP;YSIZEPERC=64"
;+++ Start the children ...
activate "UOCALDAYS".EXEC()
activate "UOTOOLCAL".EXEC()

setocc "MONTH.DUMMY", $CV_CURSELMONTH$
setocc "WEEK.DUMMY", $CV_CURSELWEEK$
fieldvideo $CV_CURSELDAY$, "COL=6"

edit</DAT>
