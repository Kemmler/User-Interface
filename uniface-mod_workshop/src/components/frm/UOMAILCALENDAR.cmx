<DAT name="GENERAL" xml:space='preserve'>entry lpBuildCalendar

   Params
      Numeric pvYear : IN
   EndParams

   Variables
      Date    lvDate
      Numeric lvYear
      Numeric lvMonth
      Numeric lvMonthNum
      Numeric lvDay
      Numeric lvDayOfWeek
      Date    lvIsToday
   EndVariables

   call lpCreateMonths

   lvDate = $date("01/01/%%pvYear") ;+++ Get first day of the year ...

   lvYear = pvYear
   setocc "MONTH.DUMMY",1	 ;+++ Start at month 1 ...
   setocc "WEEK.DUMMY",1 ;+++ Start at week 1 ...

   while (lvYear = YEAR.DUMMY)
      lvDay        = lvDate[D] ;+++ Extract day ...
      lvDayOfWeek  = lvDate[A] ;+++ Extract day-of-week ...
      WEEK_NO.WEEK = lvDate[W] ;+++ Extract week number ...

      lvIsToday = $date("%%lvDay%%%-%%lvMonth%%%-%%lvYear%%%")

      ;+++ Set day number into day-of-week for current week ...
      SelectCase lvDayOfWeek
         Case 1
            MON.WEEK = lvDay
            if ($date = lvIsToday)
               $CV_CURSELDAY$   = "MON.WEEK"
               $CV_CURSELWEEK$  = $curocc(WEEK.DUMMY)
               $CV_CURSELMONTH$ = $curocc(MONTH.DUMMY)
            endif
         Case 2
            TUE.WEEK = lvDay
            if ($date = lvIsToday)
               $CV_CURSELDAY$ = "TUE.WEEK"
               $CV_CURSELWEEK$ = $curocc(WEEK.DUMMY)
               $CV_CURSELMONTH$ = $curocc(MONTH.DUMMY)
            endif
         Case 3
            WED.WEEK = lvDay
            if ($date = lvIsToday)
               $CV_CURSELDAY$ = "WED.WEEK"
               $CV_CURSELWEEK$ = $curocc(WEEK.DUMMY)
               $CV_CURSELMONTH$ = $curocc(MONTH.DUMMY)
            endif
         Case 4
            THU.WEEK = lvDay
            if ($date = lvIsToday)
               $CV_CURSELDAY$ = "THU.WEEK"
               $CV_CURSELWEEK$ = $curocc(WEEK.DUMMY)
               $CV_CURSELMONTH$ = $curocc(MONTH.DUMMY)
            endif
         Case 5
           FRI.WEEK  = lvDay
           if ($date = lvIsToday)
              $CV_CURSELDAY$ = "FRI.WEEK"
               $CV_CURSELWEEK$ = $curocc(WEEK.DUMMY)
               $CV_CURSELMONTH$ = LVmONTH;$curocc(MONTH.DUMMY)
           endif
         Case 6
            SAT.WEEK = lvDay
            if ($date = lvIsToday)
               $CV_CURSELDAY$ = "SAT.WEEK"
               $CV_CURSELWEEK$ = $curocc(WEEK.DUMMY)
               $CV_CURSELMONTH$ = $curocc(MONTH.DUMMY)
            endif
         Case 7
            SUN.WEEK = lvDay
            if ($date = lvIsToday)
               $CV_CURSELDAY$ = "SUN.WEEK"
               $CV_CURSELWEEK$ = $curocc(WEEK.DUMMY)
               $CV_CURSELMONTH$ = $curocc(MONTH.DUMMY)
            endif
      EndSelectcase

      ;+++ After day 7 increment to following week ...
      if (lvDayOfWeek = 7) creocc "WEEK.DUMMY",-1

      lvDate  = lvDate + 1d ;+++ Increment date by 1 day ...

      lvMonth = lvDate[M] ;+++ Extract month ...

      ;+++ Check for change of month ...
      if (lvMonth != $curocc(MONTH.DUMMY))
         setocc "MONTH.DUMMY", lvMonth ;+++ Next month ...
         setocc "WEEK", 1	 ;+++ Start at week 1 ...
         lvYear  = lvDate[Y] ;+++ Extract year ...
      endif
   endwhile

   return(0)

end ; entry lpBuildCalendar

