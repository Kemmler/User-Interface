   NAME.MONTH = "JANUARY"

   creocc "MONTH.DUMMY",-1
   NAME.MONTH = "F E B R U A R Y"

   creocc "MONTH.DUMMY",-1
   NAME.MONTH = "M A R C H"

   creocc "MONTH.DUMMY",-1
   name.month = "A P R I L"

   creocc "MONTH.DUMMY",-1
   NAME.MONTH = "M A Y"

   creocc "MONTH.DUMMY",-1
   NAME.MONTH = "J U N E"

   creocc "MONTH.DUMMY",-1
   NAME.MONTH = "J U L Y"

   creocc "MONTH.DUMMY",-1
   NAME.MONTH = "A U G U S T"

   creocc "MONTH.DUMMY",-1
   NAME.MONTH = "S E P T E M B E R"

   creocc "MONTH.DUMMY",-1
   NAME.MONTH = "O C T O B E R"

   creocc "MONTH.DUMMY",-1
   NAME.MONTH = "N O V E M B E R"

   creocc "MONTH.DUMMY",-1
   NAME.MONTH = "D E C E M B E R"

   return(0)

end ; entry lpCreateMonths

