<DAT name="SFUNC" xml:space='preserve'>;------------------------------------------
; Object Service Operations
;
; IMPORTANT:
; ---------
;  Operations and parameters may change in later UNIFACE versions.
;  To stay compatible, please do not modify these operations and
;  do not call these operations directly, but only through Proc
;  statements like 'read' and 'write'.
;  Triggers and entries may be changed and new operations added.
;
; Used component variables:
;  $WHERE$     where clause of read statement of the client
;  $UWHERE$    u_where clause of read statement of the client
;  $INSTORE$   =TRUE during USTORE...UENDSTORE
;
;------------------------------------------

;------------------------------------------
operation URETR&lt;UNQ_ENTITY_1&gt;
;------------------------------------------

   ; PURPOSE;
   ; Retrieve a list of occurrences.
   ; Do not modify this operation and do not call it directly.

   ; PARAMETERS;
   ; entity &lt;ENTITY_1&gt;     : List of retrieved occurrences.
   ; occurrence &lt;ENTITY_1&gt; : Occurrence that contains the profile.
   ; FIELDS                : List of names of required fields.
   ; WHERE                 : Where-clause of the read statement
   ; UWHERE                : U_where/u_condition-clause

   ; HISTORY;
   ; Creation date         : January 4, 1999
   ; Created by            : Compuware UNIFACE Lab

   params
      entity     &lt;ENTITY_1&gt;  : OUT
      occurrence &lt;ENTITY_1&gt;  : IN
      string     FIELDS      : IN
      string     WHERE       : IN
      string     UWHERE      : IN
   endparams

   variables
      numeric    RETSTATUS
   endvariables

   $WHERE$  = WHERE           ; To be used in the READ trigger.
   $UWHERE$ = UWHERE          ; To be used in the READ trigger.

   if ($UWHERE$ != "" &amp; $WHERE$ != "")
      ; Can only happen if this operation is called directly.
      ; The read statement cannot combine uwhere and where clauses.

      call UCALLERROR("URETR&lt;UNQ_ENTITY_1&gt;", %\
         "One of the parameters 'where' and 'uwhere' must be empty.")
      return(-1)
   endif

   call URETRA("&lt;ENTITY_1&gt;", FIELDS, RETSTATUS)

   if (RETSTATUS &gt;= 0)
      ; Return only occurrences that match the profile.

      set $subsetreturn("&lt;ENTITY_1&gt;")
      if ($status &lt; 0)
         call UPROCERROR($procerrorcontext)
         return(-1)
      endif
   endif

   return(RETSTATUS)

end ; URETR&lt;UNQ_ENTITY_1&gt;

;------------------------------------------
operation USTORE&lt;UNQ_ENTITY_1&gt;
;------------------------------------------

   ; PURPOSE;
   ; Start a sequence of calls to UUPD and UREM.
   ; Do not modify this operation and do not call it directly.

   ; PARAMETERS;

   ; HISTORY;
   ; Creation date         : January 4, 1999
   ; Created by            : Compuware UNIFACE Lab

   $INSTORE$ = "T"

end ; USTORE&lt;UNQ_ENTITY_1&gt;

;------------------------------------------
operation UUPD&lt;UNQ_ENTITY_1&gt;
;------------------------------------------

   ; PURPOSE;
   ; Create or update an occurrence.
   ; Do not modify this operation and do not call it directly.

   ; PARAMETERS;
   ; FIELDS                : List of names of modified fields.
   ; occurrence &lt;ENTITY_1&gt; : New or updated occurrence.

   ; HISTORY;
   ; Creation date         : January 4, 1999
   ; Created by            : Compuware UNIFACE Lab

   params
      string FIELDS          : IN
      occurrence &lt;ENTITY_1&gt;  : IN
   endparams

   variables
      string  KEYFIELDS
      numeric RETSTATUS
   endvariables

   if (!$INSTORE$)  ; Perform only if USTORE&lt;UNQ_ENTITY_1&gt; has been called.
      call UCALLERROR("UUPD&lt;UNQ_ENTITY_1&gt;", %\
         "USTORE must be called before UUPD.")
      remocc "&lt;ENTITY_1&gt;"      ; Remove occ.param.
      return(-1)
   endif

   findkey "&lt;ENTITY_1&gt;", 1     ; Find occ with same key as occ.param.

   selectcase $status

      case -1 ; An error occurred.
         call UPROCERROR($procerrorcontext)
         remocc "&lt;ENTITY_1&gt;"   ; Remove occ.param.
         return(-1)

      case 0 ; Key does not exist.
         ; Copy the occ.param in order to initialize unmodified fields.
         creocc "&lt;ENTITY_1&gt;"
         setocc "&lt;ENTITY_1&gt;", $curocc+1           ; Setocc to occ.param.
         moveocc "&lt;ENTITY_1&gt;", $curocc-1, FIELDS  ; Move to created occ.
         if ($status &lt; 0)
            call UPROCERROR($procerrorcontext)
            remocc "&lt;ENTITY_1&gt;"                   ; Remove occ.param.
            setocc "&lt;ENTITY_1&gt;", $curocc-1
            remocc "&lt;ENTITY_1&gt;"                   ; Remove created occ.
            return(-1)
         endif

      case 1 ; Key exists in component.
         moveocc "&lt;ENTITY_1&gt;", $result, FIELDS    ; Update the found occurrence.
         if ($status &lt; 0)
            call UPROCERROR($procerrorcontext)
            remocc "&lt;ENTITY_1&gt;"   ; Remove occ.param.
            return(-1)
         endif

      case 2 ; Key exists in database.
         ; Make a new occ. with the same primary key in order to do retrieve/a.
         KEYFIELDS = $keyfields("&lt;ENTITY_1&gt;", 1)  ; Get names of primary keys.
         putlistitems/id KEYFIELDS                ; Get prim. key of occ.param.
         creocc "&lt;ENTITY_1&gt;"                      ; Create occ. for retrieve/a.
         getlistitems/id KEYFIELDS                ; Set primary key of new occ.
         call URETRA("&lt;ENTITY_1&gt;", FIELDS, RETSTATUS) ; Retrieve the occ.
         if (RETSTATUS &lt; 0)
            remocc "&lt;ENTITY_1&gt;"                   ; Remove occ. parameter.
            return(-1)
         else
            ; Update the retrieved occurrence.
            setocc "&lt;ENTITY_1&gt;", $curocc+1          ; Setocc to occ.param.
            moveocc "&lt;ENTITY_1&gt;", $curocc-1, FIELDS ; Update the retrieved occ.
            if ($status &lt; 0)
               call UPROCERROR($procerrorcontext)
               remocc "&lt;ENTITY_1&gt;"                  ; Remove occ.param.
               return(-1)
            endif
         endif

   endselectcase

   validateocc "&lt;ENTITY_1&gt;"

end ; UUPD&lt;UNQ_ENTITY_1&gt;

;------------------------------------------
operation UREM&lt;UNQ_ENTITY_1&gt;
;------------------------------------------

   ; PURPOSE;
   ; Remove an occurrence.
   ; Do not modify this operation and do not call it directly.

   ; PARAMETERS;
   ; occurrence &lt;ENTITY_1&gt; : The occurrence to be removed.

   ; HISTORY;
   ; Creation date         : January 4, 1999
   ; Created by            : Compuware UNIFACE Lab

   params
      occurrence &lt;ENTITY_1&gt;  : IN
   endparams

   variables
      numeric RETSTATUS
   endvariables

   if (!$INSTORE$)  ; Perform only if USTORE&lt;UNQ_ENTITY_1&gt; has been called.
      call UCALLERROR("UREM&lt;UNQ_ENTITY_1&gt;", %\
         "USTORE must be called before UREM.")
      remocc "&lt;ENTITY_1&gt;"      ; Remove occ.param.
      return(-1)
   endif

   findkey "&lt;ENTITY_1&gt;", 1     ; Find occ with same key as occ.param.

   selectcase $status

      case -1 ; An error occurred.
         call UPROCERROR($procerrorcontext)
         remocc "&lt;ENTITY_1&gt;"   ; Remove occ.param.
         return(-1)

      case 0 ; Key does not exist.
         ; Probably already removed by someone else, so ignore.

      case 1 ; Key exists in component.
         ; Remove occ.param and goto occ to be removed.
         moveocc "&lt;ENTITY_1&gt;", $result, ""
         if ($status &lt; 0)
            call UPROCERROR($procerrorcontext)
            return(-1)
         endif

      case 2 ; Key exists in database.
         ; Replace occ.param by database occ.
         call URETRA("&lt;ENTITY_1&gt;", "", RETSTATUS)
         if (RETSTATUS &lt; 0)
            return(RETSTATUS)
         endif

   endselectcase

   call U_REMOCC("&lt;ENTITY_1&gt;", RETSTATUS)
   return(RETSTATUS)

end ; UREM&lt;UNQ_ENTITY_1&gt;

;------------------------------------------
operation UENDSTORE
;------------------------------------------

   ; PURPOSE;
   ; End a sequence of calls to UUPD and UREM.
   ; Do not modify this operation and do not call it directly.

   ; PARAMETERS;

   ; HISTORY;
   ; Creation date         : January 4, 1999
   ; Created by            : Compuware UNIFACE Lab

   variables
      numeric RETSTATUS
   endvariables

   $INSTORE$ = "F"   ; New store sessions will be possible
                     ; from this point onwards.
   call U_STORE(RETSTATUS)
   return(RETSTATUS)

end ; UENDSTORE

;------------------------------------------
operation ULOCK&lt;UNQ_ENTITY_1&gt;
;------------------------------------------

   ; PURPOSE;
   ; Lock an occurrence.
   ; Do not modify this operation and do not call it directly.

   ; PARAMETERS;
   ; occurrence &lt;ENTITY_1&gt; : Occurrence to be locked.

   ; HISTORY;
   ; Creation date         : January 4, 1999
   ; Created by            : Compuware UNIFACE Lab

   params
      occurrence &lt;ENTITY_1&gt; : IN
   endparams

   variables
      string RETSTATUS
   endvariables

   findkey "&lt;ENTITY_1&gt;", 1         ; Find occ with same key as occ.param.

   selectcase $status

      case -1 ; An error occurred.
         call UPROCERROR($procerrorcontext)
         remocc "&lt;ENTITY_1&gt;"       ; Remove occ. parameter.
         return(-1)

      case 0 ; Key does not exist.
         call UCALLERROR("ULOCK&lt;UNQ_ENTITY_1&gt;", "Occurrence does not exist.")
         remocc "&lt;ENTITY_1&gt;"       ; Remove occ. parameter.
         return(-1)

      case 1 ; Key exists in component.
         ; Remove occ.param and goto occ to be locked.
         moveocc "&lt;ENTITY_1&gt;", $result, ""
         if ($status &lt; 0)
            call UPROCERROR($procerrorcontext)
            remocc "&lt;ENTITY_1&gt;"    ; Remove occ. parameter.
            return(-1)
         endif

      case 2 ; Key exists in database.
         ; Replace occ.param by database occ..
         call URETRA("&lt;ENTITY_1&gt;", "", RETSTATUS)
         if (RETSTATUS &lt; 0)
            return(RETSTATUS)
         endif

   endselectcase

   call U_LOCK(RETSTATUS)
   return(RETSTATUS)

end ; ULOCK&lt;UNQ_ENTITY_1&gt;

;------------------------------------------
operation UCOMMIT
;------------------------------------------

   ; PURPOSE;
   ; Commit the current transaction.
   ; Do not modify this operation and do not call it directly.

   ; PARAMETERS;

   ; HISTORY;
   ; Creation date         : January 4, 1999
   ; Created by            : Compuware UNIFACE Lab

   variables
      numeric RETSTATUS
   endvariables

   call U_COMMIT(RETSTATUS)
   return(RETSTATUS)

end ; UCOMMIT

;------------------------------------------
operation UROLLBACK
;------------------------------------------

   ; PURPOSE;
   ; Rollback the current transaction and clean up the component.
   ; Do not modify this operation and do not call it directly.

   ; PARAMETERS;

   ; HISTORY;
   ; Creation date         : January 4, 1999
   ; Created by            : Compuware UNIFACE Lab

   variables
      numeric RETSTATUS
   endvariables

   call U_ROLLBACK(RETSTATUS)
   return(RETSTATUS)

end ; UROLLBACK
