<DAT name="GENERAL" xml:space='preserve'>;------------------------------------------
entry UPROCERROR
;------------------------------------------

   ; PURPOSE;
   ; Handle a failure in a Proc statement.
   ; A message in English is written to the message frame.
   ; It is advised to remove or change this in end-user applications.

   ; PARAMETERS;
   ; CONTEXT               : $procerrorcontext.

   ; HISTORY;
   ; Creation date         : January 4, 1999
   ; Created by            : Compuware UNIFACE Lab
   ; 
   ; Change date           : 
   ; Changed by            : 
   ; Description of change : 

   params
      string   CONTEXT    : IN
   endparams

   variables
      string   ERROR
      string   OPERNAME
      string   DESCR
   endvariables

   getitem/id ERROR,    context, "ERROR"
   getitem/id OPERNAME, context, "PROCNAME"
   getitem/id DESCR,    context, "DESCRIPTION"
   putmess "Error detected by object service. %%DESCR%%% (%%ERROR%%%)"

end ; UPROCERROR

;------------------------------------------
entry UCALLERROR
;------------------------------------------

   ; PURPOSE;
   ; Handle an incorrect call to an operation.
   ; A message in English is written to the message frame.
   ; It is advised to remove or change this in end-user applications.

   ; PARAMETERS;
   ; OPERNAME              : Operation name.
   ; ERRORDESC             : Error description.

   ; HISTORY;
   ; Creation date         : January 4, 1999
   ; Created by            : Compuware UNIFACE Lab
   ; 
   ; Change date           : 
   ; Changed by            : 
   ; Description of change : 

   params
      string   OPERNAME   : IN
      string   ERRORDESCR : IN
   endparams

   putmess "Object service operation %%OPERNAME%%% failed. %%ERRORDESCR%%%"

end ; UCALLERROR

;------------------------------------------
entry URETRA
;------------------------------------------

   ; PURPOSE;
   ; Retrieve an extra occurrence.
   ; This entry can be modified to customize the object service.

   ; PARAMETERS;
   ; ENTNAME               : Entity name.
   ; FIELDS                : Required fields.
   ; RETSTATUS             : Success status.

   ; HISTORY;
   ; Creation date         : January 4, 1999
   ; Created by            : Compuware UNIFACE Lab
   ; 
   ; Change date           : 
   ; Changed by            : 
   ; Description of change : 

   params
      string   ENTNAME   : IN
      string   FIELDS    : IN
      numeric  RETSTATUS : OUT
   endparams

   $selectlist(ENTNAME) = FIELDS ; Only retrieve the required fields.
   retrieve/a ENTNAME            ; Use curr.occ. as a profile to retrieve.
   RETSTATUS = $status
   if (RETSTATUS &lt; 0)
      call UPROCERROR($procerrorcontext)
      remocc ENTNAME             ; Remove profile.
   endif

end ; URETRA

