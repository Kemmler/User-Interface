		string toHex, sHex
	endvariables
	
	toHex = "0=0&uSEP;1=1&uSEP;2=2;3=3&uSEP;4=4&uSEP;5=5&uSEP;6=6&uSEP;7=7&uSEP;8=8&uSEP;9=9&uSEP;A=10&uSEP;B=11&uSEP;C=12&uSEP;D=13&uSEP;E=14&uSEP;F=15"
	
	if (pHex[1,2] = "&amp;H")
		pHex = pHex[3]
	endif

	nPos = 1 
	nPow = $length(pHex) - 1

	while (nPos &lt;= $length(pHex))

		sHex = pHex[nPos:1]
		nHex += $number($item(sHex, toHex)) * $power(16, nPow)
		nPos += 1
		nPow -= 1

	endwhile	

	return nHex
	
end ; entry numToHex


; *** Convert Numeric to Hex
entry numToHex
	returns string
	
	params
		numeric pNumeric : IN
	endparams
	
	variables
		numeric nNumeric
	endvariables

	nNumeric = pNumeric % 16
	if (pNumeric - nNumeric = 0)
		return toChar(nNumeric)
	else
		return $concat(numToHex((pNumeric - nNumeric) / 16), toChar(nNumeric))
	endif
	
end ; entry numToHex


entry toChar

	returns string

	params
		numeric pNumeric : IN
	endparams

	variables
		string toHex
	endvariables

	toHex = "0&uSEP;1&uSEP;2&uSEP;3&uSEP;4&uSEP;5&uSEP;6&uSEP;7&uSEP;8&uSEP;9&uSEP;A&uSEP;B&uSEP;C&uSEP;D&uSEP;E&uSEP;F"
	return $itemnr(pNumeric + 1, toHex)

end ; entry toChar
