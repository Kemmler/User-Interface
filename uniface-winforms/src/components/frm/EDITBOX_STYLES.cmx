<DAT name="ASYNC" xml:space='preserve'>variables
	numeric hWnd, dwNewLong, iTextSize
	string name
endvariables

selectcase $msgid
	case "FGF"
		; Get Window Handle of active Window;
		; *** WARNING: This will not work correctly when Debugger is used!!!
		activate "USER32".GetFocus()
		hWnd = $status
		; Does the active window belong to the active field? ($msgdata = $fieldname)
		activate "YRTL".ugetname(hWnd, name, 255)
		if ($scan(name, $msgdata) &gt; 0)
			; Are we here for the first time?
			if ($item("defstyle", $fieldproperties) = "")
				; If yes, then save the default style of the Edit Box
				activate "USER32".GetWindowLong(hWnd, "&lt;GWL_STYLE&gt;")
				dwNewLong = $status
				putitem/id $fieldproperties, "defstyle", dwNewLong
				; And we check the default text length of the Edit Box
				activate "USER32".SendMessage(hWnd, hexToNum("&lt;EM_GETLIMITTEXT&gt;"), 0, 0)
				iTextSize = $status
				putitem/id $fieldproperties, "TextSize", iTextSize
			else
				; If no, then restore the default style of the Edit Box
				dwNewLong = $number($item("defstyle", $fieldproperties))
				activate "USER32".SetWindowLong(hWnd, "&lt;GWL_STYLE&gt;", dwNewLong)
				; And we reset the default length of the Edit Box
				iTextSize = $number($item("TextSize", $fieldproperties))
				activate "USER32".SendMessage(hWnd, hexToNum("&lt;EM_SETLIMITTEXT&gt;"), iTextSize, 0)
			endif
			show
			; Now we check the chosen "Style"
			selectcase FLD1.DUM
				case "N"
					; If style is "N" then we can only enter numbers into the Edit Box
					dwNewLong = $number($item("defstyle", $fieldproperties)) + hexToNum("&lt;ES_NUMBER&gt;")
					activate "USER32".SetWindowLong(hWnd, "&lt;GWL_STYLE&gt;", dwNewLong)
				case "L"
					; If style is "L" then we can only enter lowercase text into the Edit Box
					dwNewLong = $number($item("defstyle", $fieldproperties)) + hexToNum("&lt;ES_LOWERCASE&gt;")
					activate "USER32".SetWindowLong(hWnd, "&lt;GWL_STYLE&gt;", dwNewLong)
				case "U"
					; If style is "U" then we can only enter uppercase text into the Edit Box
					dwNewLong = $number($item("defstyle", $fieldproperties)) + hexToNum("&lt;ES_UPPERCASE&gt;")
					activate "USER32".SetWindowLong(hWnd, "&lt;GWL_STYLE&gt;", dwNewLong)
			endselectcase
			; And we change the text lenght according to value of FLD2 (if &gt; 0)
			if (FLD2.DUM &gt; 0)
				activate "USER32".SendMessage(hWnd, hexToNum("&lt;EM_SETLIMITTEXT&gt;"), FLD2.DUM, 0)
			endif
		else
			; Soemthing went wrong! The specified field cannot be found!?! Is the Debugger active?
			message "ERROR: Could not set Style; field not found!"
		endif
endselectcase
</DAT>
