<DAT name="INIT" xml:space='preserve'>variables
	numeric appHndl,mdihwnd, frmhndl
	string vhref
endvariables
show
$applproperties = "messageline=false&uSEP;backcolor=#646464"
appHndl = Ugetapphndl()
if(!utransparency(appHndl, "0x64","0x64","0x64")) Message/info "Set Transparency failed -:%%$status" ;Make transparent using background hex values RGB #646464

frmhndl = findhWndByTitle(uGetDesktopWindow(),"UniForm",$instancename)
