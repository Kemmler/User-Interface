<DAT name="GENERAL" xml:space='preserve'>#include WINAPI:USER32_FUNCTIONS
#include WINAPI:UINTERFACE



entry getHwndlist
	params
		numeric phWnd : IN
		string pClassName : IN
		string pWinTitle : IN
	endparams
	
	variables
		numeric hUniWnd, hWnd, hWndChild
		string lpClassName, lpString
		Raw lpRaw
	endvariables
;	debug
	if(phWnd = 0)
		hUniWnd = 0
		activate "YRTL".ugethwnd()
		hUniWnd = $status
	else
		hUniWnd = phWnd
	endif
	hWnd = 0
	activate "USER32".GetWindow(hUniWnd, &lt;GW_CHILD&gt;)
	hWnd=$status
	activate "USER32".GetWindow(hWnd, &lt;GW_HWNDFIRST&gt;)
	hWnd=$status
	while(hWnd)
		
		
		lpClassName = ""
		
		activate "USER32".getClassName(hWnd, lpClassName, 511)
;		if(lpClassName = pClassName)
			activate "USER32".GetWindowTextRaw(hWnd, lpRAW, 511)
			lpString = ""
			activate "USER32".GetWindowText(hWnd, lpString, 511)
			list="%%list%%%%%^DEC-&gt;%%hWnd%%% HEX-&gt;%%(numtohex(hWnd))%%% Class-&gt;%%lpClassName%%%-&gt; Title-&gt;%%lpString%%%"
			if(pWinTitle != "")
				if(lpString = pWinTitle)
					return hWnd
				endif
			endif
;		endif
		hWndChild=getHwndlist(hWnd, pClassName, pWinTitle)
		if(hwndChild &gt; 0)
			return hWndChild
		endif

		activate "USER32".getWindow(hWnd, &lt;GW_HWNDNEXT&gt;)
		hwnd = $status
	endwhile
	return(-1)
	
end


