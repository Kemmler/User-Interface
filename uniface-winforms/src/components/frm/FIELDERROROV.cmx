<DAT name="INIT" xml:space='preserve'>params
string pErrorMsg :IN
endparams
show
$hWnd$ = getUformHwnd(0,"%%$instancename%%%")
call utransparency($hWnd$, "0x64","0x64","0x64");#646464

$fieldproperties("TIMER") = "UseFieldValue=false"
;Dump the JavaScript timer code into the embedded container and start the countdown to remove form
$fieldhandle("TIMER")-&gt;$widgetoperation("loadHTML", settimer())
t1=$text("%%pErrorMsg%%%")
$focus$=0
edit
</DAT>
