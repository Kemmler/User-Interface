<DAT name="GENERAL" xml:space='preserve'>#include WINAPI:USER32_FUNCTIONS
#include WINAPI:UINTERFACE
;-----------Build Javascript for timer-----------------
entry settimer
	returns string
	variables
		string vHTML
	endvariables
	
	vHTML=$timer
	
	return(vHTML)
	
timer:blockdata ~
&lt;script language="Javascript"&gt;
var countdown;
var countdown_number;
function countdown_init() {
    countdown_number = 9;
    countdown_trigger();
}
function countdown_trigger() {
    if(countdown_number &gt; 0) {
        countdown_number--;
        //document.getElementById('countdown_text').innerHTML = countdown_number;
            countdown = setTimeout('countdown_trigger()', 500);
    }
    else
    {
    window.unifaceTriggers('TimeOut');
    }
}
function countdown_clear() {
    clearTimeout(countdown);
}
window.unifaceTriggers('Start');
&lt;/script&gt;
&lt;body leftmargin="0px" topmargin="0px" marginwidth="0px" marginheight="0px" style="overflow: hidden;height: 1px; width: 1px;"&gt;
&lt;div id="countdown_text" style="height: 1px; width: 1px; valign: top;"&gt; &lt;/div&gt;
&lt;/body&gt;
~
end
;-----------Build Javascript for timer-----------------
</DAT>
