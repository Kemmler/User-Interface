<DAT name="UFTRIGGERS" xml:space='preserve'>; -- start triggers for tree widget 

trigger Expand
	params
		string control: in ; TREE | LIST | KEYBOARD
		string item: in
	endparams
	
end ; trigger Expand

trigger Collapse
	params
		string control: in ; TREE | LIST | KEYBOARD
		string item: in
	endparams
	
end ; trigger Collapse

trigger Open
	params
		string control: in ; TREE | LIST | KEYBOARD
		string item: in
	endparams
	
end ; trigger Open

trigger Close
	params
		string control: in ; TREE | LIST | KEYBOARD
		string item: in
	endparams
	
end ; trigger Close

trigger doubleClick
	params
		string control: in ; TREE | LIST | KEYBOARD
		string item: in
	endparams
	
end ; trigger doubleClick

trigger columnSorted
	params
		numeric columnId: in
		string sortingOrder: in ; ASC | DESC
	endparams
	
	variables
		numeric hWnd
		string lpClassName
		string vr1
	endvariables
debug
vr1 = $sortlist($valrep(tree1), "$string(1):a")
	$valrep(tree1)=vr1
	activate "USER32".GetFocus()
	hWnd = $status
	activate "USER32".GetClassName(hWnd, lpClassName, 255)
	if (lpClassName = "SysListView32")
		FLD3.DUM = hWnd
		activate "USER32".SendMessage(hWnd, &lt;LVM_GETSELECTIONMARK&gt;, 0, 0)
		FLD1.DUM = $status + 1
		activate "USER32".SendMessage(hWnd, &lt;LVM_GETITEMCOUNT&gt;, 0, 0)
		FLD2.DUM = $status
	endif
	
end ; trigger columnSorted

trigger columnWidth
	params
		numeric id: in
		numeric width: in
	endparams
	
end ; trigger columnWidth

trigger representationChanging
	params
		string itemValue: in
	endparams
	
end ; trigger representationChanging

trigger representationChanged ; Only works if tree widget property INLINEEDIT=TRUE
	params
		string itemValue: in
		string control: in ; TREE | LIST
		string newRepresentation: in
	endparams
	variables
		string treeItem, listUpdate
	endvariables
	
	getitem/id treeItem, $valrep, itemValue
	putitem treeItem, 3, newRepresentation       ; Change representation
	listUpdate = "0="                            ; Indicate partial valrep update
	putitem/id listUpdate, itemValue, treeItem
	$valrep = listUpdate
	
end ; trigger representationChanged

; start drag &amp; drop triggers for tree widget

trigger moveTo
	params
		string dragvalue: in
		string dropformat: in
		string itemvalue: in
		numeric numberofobjects: in
		numeric index: in
	endparams
	
end ; trigger moveTo

trigger moveFrom
	params
		string dragvalue: in
		string dropformat: in
		numeric numberofobjects: in
		numeric index: in
	endparams
	
end ; trigger moveFrom

trigger moveWithin
	params
		string dragvalue: in
		string dropformat: in
		string itemvalue: in
		numeric numberofobjects: in
		numeric index: in
	endparams
	
end ; trigger moveWithin

trigger copyTo
	params
		string dragvalue: in
		string dropformat: in
		string itemvalue: in
		numeric numberofobjects: in
		numeric index: in
	endparams
	
end ; trigger copyTo

trigger createShortcutTo
	params
		string dragvalue: in
		string dropformat: in
		string itemvalue: in
		numeric numberofobjects: in
		numeric index: in
	endparams
	
end ; trigger createShortcutTo

; -- end triggers for tree widget
