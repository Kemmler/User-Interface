<DAT name="ASYNC" xml:space='preserve'>variables
	numeric hWnd
	string lpClassName
endvariables

if ($msgid = "GetFocus")
	activate "USER32".GetFocus()
	hWnd = $status
	activate "USER32".FindWindowEx(hWnd, 0, $msgdata, 0)
	hWnd = $status
	if (hWnd &gt; 0)
		FLD3.DUM = hWnd
		activate "USER32".SendMessage(hWnd, &lt;LVM_GETSELECTIONMARK&gt;, 0, 0)
		FLD1.DUM = $status + 1
		activate "USER32".SendMessage(hWnd, &lt;LVM_GETITEMCOUNT&gt;, 0, 0)
		FLD2.DUM = $status
	else
		message/hint "FindWindowEx could not find %%$msgdata"
	endif
endif
