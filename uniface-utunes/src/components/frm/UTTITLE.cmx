<DAT name="HTML_CTRLTYPE">U_INPUT</DAT>
<DAT name="UFTRIGGERS" xml:space='preserve'>trigger uplay
	variables
		handle controlhandle
		numeric playstate
	endvariables

	$hplayer$-&gt;get_playstate(playstate)
	$hplayer$-&gt;get_controls(controlhandle)
	;set the details for title worker
	selectcase playstate
		case 10,0,1,2
			activate "UTTITLE".setworker()
			controlhandle-&gt;play()
			$fieldhandle(TITLEWORKER)-&gt;$widgetoperation("JS:setTitle", $cname$, $cartist$)
		case 3
			controlhandle-&gt;pause()
		elsecase
	endselectcase
end

trigger uvolume
	params
		numeric pVolume: IN
	endparams
	variables
		handle settingshandle
	endvariables
	$hplayer$-&gt;get_settings(settingshandle)
	settingshandle-&gt;set_volume(pVolume)
end
trigger unext


