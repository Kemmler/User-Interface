<DAT name="INIT" xml:space='preserve'>variables
handle settings_handle
numeric FormhWnd
endvariables

activate "UTSEARCH".exec()


$fieldproperties(CLOSE_BTN)="ATTACH=HMOVE"
$fieldproperties(MAX_BTN)="ATTACH=HMOVE"
$fieldproperties(MIN_BTN)="ATTACH=HMOVE"
$fieldproperties(SEARCH)="ATTACH=HMOVE"
$fieldproperties(TITLEWORKER)="ATTACH=HMOVE"

show
$hwnd$ = Ugetapphndl()
call getme($hwnd$,FormhWnd)

activate "UTUNES".hookwindow(FormhWnd)

$hplayer$ = $ocxhandle ("PLAYER")
$hplayer$-&gt;get_settings(settings_handle)
settings_handle-&gt;set_autostart(0)
search="UTSEARCH"

edit</DAT>
<DAT name="CLEAR" xml:space='preserve'>#ifdefined frmEntity
  ; Generated three tier component: Delegate clear() to FRM Subtype.
  ; This allows a uniform implementation that can be invoked
  ; by other objects too (for example, by a 'Clear' Button).
  $collhandle("&lt;frmEntity&gt;")-&gt;clear()

#else
  clear

#endif
</DAT>
<DAT name="RETRIEVE" xml:space='preserve'>#ifdefined frmEntity
  ; Generated three tier component: Delegate load() to FRM Subtype.
  ; This allows a uniform implementation that can be invoked
  ; by other objects too (for example, by a 'Load' Button).
  $collhandle("&lt;frmEntity&gt;")-&gt;load()

#else
  retrieve
  if ($status &lt; 0)
    message $text(1762) ;error
  endif

#endif</DAT>
<DAT name="RECORD" xml:space='preserve'>#ifdefined frmEntity
  ; Generated three tier component: Delegate load() to FRM Subtype.
  ; This allows a uniform implementation that can be invoked
  ; by other objects too (for example, by a 'Load' Button).
  $collhandle("&lt;frmEntity&gt;")-&gt;load()

#else
  retrieve
  if ($status &lt; 0)
    message $text(1762) ;error
  endif

#endif</DAT>
<DAT name="STORE" xml:space='preserve'>#ifdefined frmEntity
  ; Generated three tier component: Delegate save() to FRM Subtype.
  ; This allows a uniform implementation that can be invoked
  ; by other objects too (for example, by a 'Save' Button).
  $collhandle("&lt;frmEntity&gt;")-&gt;save()

#else
  store
  if ($status &lt; 0)
     message $text(1500)    ; error
     rollback
  else
    if ($status = 1)
      message $text(1723)   ; no change
    else
      commit
      if ($status &lt; 0)
        rollback
      else
        message $text(1805) ; Ok
      endif
    endif
  endif

#endif</DAT>
<DAT name="DELET" xml:space='preserve'>#ifdefined frmEntity
  ; Generated three tier component: Delegate deleteall() to FRM Subtype.
  ; This allows a uniform implementation that can be invoked
  ; by other objects too (for example, by a 'Delete All' Button).
  $collhandle("&lt;frmEntity&gt;")-&gt;deleteall()

#else
  erase
  if ($status &lt; 0)
    message $text(1763)   ;error
    rollback
  else
    if ($status = 1)
      message $text(1634) ;not allowed
    else
      message $text(1806) ;Ok
      commit
    endif
  endif

#endif</DAT>
<DAT name="ACCEPT" xml:space='preserve'>#ifdefined frmEntity
  ; Generated three tier component: Delegate save() to FRM Subtype.
  ; This allows a uniform implementation that can be invoked
  ; by other objects too (for example, by an 'OK' Button).
if ($formmod)
  $collhandle("&lt;frmEntity&gt;")-&gt;save()       ; save modifications
  if ($status &lt; 0)                         ; check negative status from Save()
    return (-1)
  endif
endif

exit (0)
#else
#endif</DAT>
<DAT name="GENERAL" xml:space='preserve'>#include WINAPI:USER32_FUNCTIONS
#include WINAPI:UINTERFACE

entry setmaxbutton
	variables
		numeric winstyle
	endvariables
	
	
	activate "USER32".Iszoomed($hwnd$)
	winstyle = $status
	if(winstyle)  ;maximized
      
		putitem/id $fieldproperties("MAX_BTN"), "ImgDefault", "@max.png"
		putitem/id $fieldproperties("MAX_BTN"), "ImgSelect", "@max.png"
		putitem/id $fieldproperties("MAX_BTN"), "ImgFocus", "@max.png"
		putitem/id $fieldproperties("MAX_BTN"), "ImgHover", "@max_hover.png"
		activate "USER32".showwindow($hwnd$,&lt;SW_RESTORE&gt;)
	else
		putitem/id $fieldproperties("MAX_BTN"), "ImgDefault", "@restore.png"
		putitem/id $fieldproperties("MAX_BTN"), "ImgSelect", "@restore.png"
		putitem/id $fieldproperties("MAX_BTN"), "ImgFocus", "@restore.png"
		putitem/id $fieldproperties("MAX_BTN"), "ImgHover", "@restore_hover.png"
		activate "USER32".showwindow($hwnd$,&lt;SW_MAXIMIZE&gt;)
	endif

entry getme
params
numeric pUniWnd :IN
numeric phWnd: OUT
endparams
variables
numeric hwnd
string vclass, vtext
endvariables
	hWnd = 0
	activate "USER32".GetWindow(pUniWnd, &lt;GW_CHILD&gt;)
	hWnd=$status
	activate "USER32".GetWindow(hWnd, &lt;GW_HWNDFIRST&gt;)
	hWnd=$status
	activate "USER32".GetWindow(hWnd, &lt;GW_CHILD&gt;)
	hWnd=$status
	activate "USER32".GetWindow(hWnd, &lt;GW_HWNDFIRST&gt;)
