;show
end
;------------------------------------------------------------------------
operation showcenterpanel
params
string pcomponent:  IN
string pID: IN
endparams
;newinstance pcomponent, pcomponent
activate pcomponent.execu(pID)
centerpanel = pComponent
;;$windowproperties("UOBODY","splitbar")="splitbar=S3:MAX"
end
;------------------------------------------------------------------------
operation showlowercenterpanel
params
string pcomponent:  IN
string pID: IN
endparams
activate "UOLOWERPANEL".setcomponents(pcomponent,pID)
lowercenterpanel="UOLOWERPANEL"
$windowproperties("UOBODY","splitbar")="splitbar=S3:RESTORE"
$lowerpanelvisible$ = 1
;show
end
;------------------------------------------------------------------------
operation hidelowercenterpanel
$windowproperties("UOBODY","splitbar")="splitbar=S3:MAX"
$lowerpanelvisible$ = 0
;show
end
;------------------------------------------------------------------------
operation maxlowercenterpanel
$windowproperties("UOBODY","splitbar")="splitbar=S3:MIN"
$lowerpanelvisible$ = 2
;show
end
;------------------------------------------------------------------------
operation enablelowerpanel
$windowproperties("UOBODY","splitbar")="splitbar=S3:RESTORE"
$lowerpanelvisible$ = 1
end
;------------------------------------------------------------------------

;------------------------------------------------------------------------
operation rightvisible
params
numeric pParm: OUT
endparams
pParm=$rightpanelvisible$
end
;------------------------------------------------------------------------
operation disablerightpanel
$rightpanelvisible$ = 0
$windowproperties("UOPANELS","splitbar")="splitbar=S2:MAX"
$windowproperties("UOBODY","splitbar")="splitbar=S0:MAX"
end
;------------------------------------------------------------------------
operation enablerightpanel
$rightpanelvisible$ = 1
$windowproperties("UOBODY","splitbar")="splitbar=S0:RESTORE"
$windowproperties("UOPANELS","splitbar")="splitbar=S2:MAX"
end
;------------------------------------------------------------------------
operation minimizerightpanel
$rightpanelvisible$ = 2
$windowproperties("UOPANELS","splitbar")="splitbar=S2:RESTORE"
$windowproperties("UOBODY","splitbar")="splitbar=S0:MAX"
end
;------------------------------------------------------------------------
operation lowerpanelvisible
params
numeric pParm: OUT
endparams
pParm=$lowerpanelvisible$
end
;------------------------------------------------------------------------
operation leftvisible
params
