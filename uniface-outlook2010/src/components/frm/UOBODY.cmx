endparams
pParm=$leftpanelvisible$
end
;------------------------------------------------------------------------
operation minimizeleftpanel
$leftpanelvisible$ = 2
$windowproperties("UOPANELS","splitbar")="splitbar=S1:RESTORE"
$windowproperties("UOBODY","splitbar")="splitbar=S1:MIN"
fieldsyntax "LEFTCOLLAPSE", "HID"
end
;------------------------------------------------------------------------
