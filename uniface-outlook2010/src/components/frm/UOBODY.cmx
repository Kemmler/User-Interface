<DAT name="SFUNC" xml:space='preserve'>operation take
	params
		string pInstance: IN
		string pPosn: IN
	endparams
	if(pPosn = "R")
		activate pInstance.execu(0,pPosn)
		rightpanel = pInstance
	elseif(pPosn = "L")
		activate pInstance.execu(0,pPosn)
		leftpanel = pInstance
	endif
;	show
end
;------------------------------------------------------------------------
operation execu
	if(!$interactive)
		edit/nonmodal
	endif
end
;------------------------------------------------------------------------
operation showcomponent
	params
		string pInstance: IN
		boolean pType:  IN
	endparams
	activate $instancename.execu()
	activate pInstance.execu(pType)
	rightpanel = pInstance
;	show
end
operation showleftbutton
